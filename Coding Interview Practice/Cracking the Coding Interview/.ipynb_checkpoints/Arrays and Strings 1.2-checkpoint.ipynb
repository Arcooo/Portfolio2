   ],
   "source": [
    "def checkPermuations(string1, string2):\n",
    "    if (len(string1) != len(string2)):\n",
    "        return False\n",
    "    \n",
    "    if sorted(string1) != sorted(string2):\n",
    "        return False\n",
    "    \n",
    "    return True\n",
    "\n",
    "checkPermuations('abc', 'bac')"
   ]
  }
